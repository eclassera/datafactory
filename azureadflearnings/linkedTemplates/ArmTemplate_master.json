{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azureadflearnings"
		},
		"AzureBlobStorage_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage_ls'"
		},
		"AzureDatabricks_ls_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks_ls'"
		},
		"AzureDataExplorer_linkservice_properties_typeProperties_endpoint": {
			"type": "string",
			"defaultValue": "https://azureadxtraining.eastus2.kusto.windows.net"
		},
		"AzureDataExplorer_linkservice_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "mra_dwh"
		},
		"AzureDataLakeStorage_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azuredataadlsdev.dfs.core.windows.net/"
		},
		"AzureKeyVault_training_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://azure-east-kv.vault.azure.net/"
		},
		"TransientStorage_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://banoriontransient.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azureadflearnings_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage_ls_connectionString": {
						"value": "[parameters('AzureBlobStorage_ls_connectionString')]"
					},
					"AzureDatabricks_ls_accessToken": {
						"value": "[parameters('AzureDatabricks_ls_accessToken')]"
					},
					"AzureDataExplorer_linkservice_properties_typeProperties_endpoint": {
						"value": "[parameters('AzureDataExplorer_linkservice_properties_typeProperties_endpoint')]"
					},
					"AzureDataExplorer_linkservice_properties_typeProperties_database": {
						"value": "[parameters('AzureDataExplorer_linkservice_properties_typeProperties_database')]"
					},
					"AzureKeyVault_training_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_training_properties_typeProperties_baseUrl')]"
					},
					"TransientStorage_ls_properties_typeProperties_url": {
						"value": "[parameters('TransientStorage_ls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "azureadflearnings_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage_ls_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_ls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'azureadflearnings_ArmTemplate_0')]"
			]
		},
		{
			"name": "azureadflearnings_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'azureadflearnings_ArmTemplate_1')]"
			]
		},
		{
			"name": "azureadflearnings_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'azureadflearnings_ArmTemplate_2')]"
			]
		}
	]
}